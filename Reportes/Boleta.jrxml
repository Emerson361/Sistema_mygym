	<parameter name="codreserva" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[select c.nombre, c.Apellido, c.doc_num, c.telefono, c.correo, c.genero,
	det.horario, det.fecha_inicio, det.fecha_final, m.tipo_membresia, m.precio,
    d.Nombre as nombre_dieta, r.Nombre as nombre_rutina
from ventas v
inner join cliente c on v.Id_cliente = c.id_cliente
inner join membresia m on v.Id_membresia = m.id_membresia
inner join detalle_membresia det on m.detalle_mem_id = det.id_detalle_mem
inner join dieta d on v.Id_dieta = d.Id_dieta
inner join rutina r on v.Id_rutina = r.id_rutina where v.id_ventas = $P!{codreserva}]]>
	</queryString>
